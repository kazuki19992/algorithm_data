#include<stdio.h>

#define SIZE 5
#define NO_DATA -1      // ãƒ‡ãƒ¼ã‚¿ãªã—

int queue[SIZE] = {NO_DATA};  // ãƒªãƒ³ã‚°ãƒãƒƒãƒ•ã‚¡
int front = -1;                     // ãƒ•ãƒ­ãƒ³ãƒˆã‚«ãƒ¼ã‚½ãƒ«
int rear = -1;                      // ãƒªã‚¢ã‚«ãƒ¼ã‚½ãƒ«
int num = 0;                        // ã‚­ãƒ¥ãƒ¼ã®ãƒ‡ãƒ¼ã‚¿æ•°

int EnQueue(int data);
int DeQueue();
void ShowQueue();

int stack[SIZE];  // ã‚¹ã‚¿ãƒƒã‚¯ã®å®Ÿæ…‹ã®é…åˆ—
int sp = -1;            // ã‚¹ã‚¿ãƒƒã‚¯ãƒã‚¤ãƒ³ã‚¿(ãƒˆãƒƒãƒ—)

int Push(int data);     // ãƒ—ãƒƒã‚·ãƒ¥
int Pop();              // ãƒãƒƒãƒ—
void ShowStack();       // ã‚¹ã‚¿ãƒƒã‚¯è¡¨ç¤º

int main() {
    int data,select;

    for(int i = 0; i < SIZE; i++){
        queue[i] = NO_DATA;
    }
    
    while(1){
        printf("(1) Push (2) Pop (3) Enq (4) Deq (5) Push(Deq) (6) Enq(Pop) (0) Exit :");
        scanf("%d",&select);

        switch(select){
            case 1: {       // Push
                printf("Data : ");
                scanf("%d",&data);      // Pushã™ã‚‹ãƒ‡ãƒ¼ã‚¿ã®å…¥åŠ›

                if(Push(data) == NO_DATA){
                    printf("Stack Overflow!\n");
                }else{
                    ShowStack();
                }
                break;
            }

            case 2: {       // Pop
                if(Pop() == NO_DATA){
                    printf("Stack Pop is failed!\n");
                }else{
                    ShowStack();
                }
                break;
            }

            case 3: {       // Enq
                printf("Data? :");
                scanf("%d",&data);
                if(EnQueue(data) == -1){
                    printf("Enqueue is failed!\n");
                }
                ShowQueue();
                break;
            }

            case 4:{        // Deq
                if(DeQueue() == -1){
                    printf("Dequeue is failed!\n");
                    continue;
                }
                ShowQueue();
                break;
            }

            case 5:{        // Push(Deq)
                int tmp = DeQueue();
                if(tmp == -1){
                    printf("Dequeue is failed!\n");
                    continue;
                }
                if(Push(tmp) == NO_DATA){
                    printf("Stack Overflow!\n");
                }else{
                    ShowQueue();
                    ShowStack();
                }
                break;
            }

            case 6:{        // Enq(Pop)
                int tmp = Pop();
                if(tmp == NO_DATA){
                    printf("Stack Pop is failed!\n");
                }
                if(EnQueue(tmp) == -1){
                    printf("Enqueue is failed!\n");
                }
                ShowStack();
                ShowQueue();
                break;
            }

            case 0: {       // çµ‚äº†
                return 0;   
            }
        }
    }
}

void ShowQueue(){
    printf("Queue : ");
    for(int i = 0; i < SIZE; i++){
        if (queue[i] != NO_DATA){   // æœ‰åŠ¹ãƒ‡ãƒ¼ã‚¿
            printf("[%d]",queue[i]);
        }else{
            printf("[   ]");        // ç„¡åŠ¹ãƒ‡ãƒ¼ã‚¿
        }
    }
    putchar('\n');
}

int EnQueue(int data){
    int temp;
    temp = rear;
    rear++;
    if(rear >= SIZE){
        rear = 0;                   // ãƒªãƒ³ã‚°ãƒãƒƒãƒ•ã‚¡
    }
    if( num >= SIZE){
        printf("Queue is Full!\n");
        rear = temp;
        return -1;
    }else{
        queue[rear] = data;
        num++;
        return 0;
    }
}

int DeQueue(){
    int temp, data = 0;
    temp = front;
    front++;
    if(front >= SIZE){
        front = 0;
    }
    if(num == 0){
        printf("Queue is Empty!\n");
        front = temp;
        return -1;
    }else{
        data = queue[front];
        queue[front] = NO_DATA;
        num--;
        printf("Dequeue data is %d\n",data);
        return data;
    }
}

void ShowStack(){
    printf("Stack : ");
    for(int i = 0; i <= sp; i++){   // ã‚¹ã‚¿ãƒƒã‚¯ãƒã‚¤ãƒ³ã‚¿(ãƒœãƒˆãƒ ã‹ã‚‰ãƒˆãƒƒãƒ—ã¾ã§è¡¨ç¤º)
        printf("[%d]",stack[i]);
    }
    putchar('\n');
}

int Push(int data){
    sp++;   // ã‚¹ã‚¿ãƒƒã‚¯ãƒã‚¤ãƒ³ã‚¿ +1
    if(sp >= SIZE){
        sp--;
        return -1;
    }else{
        stack[sp] = data;   // ãƒ‡ãƒ¼ã‚¿ä»£å…¥
        return 0;   // æ­£å¸¸çµ‚äº†
    }
}

int Pop() {
    int data = 0;   // ãƒ‡ãƒ¼ã‚¿åˆæœŸåŒ–

    if(sp < 0){     // ã‚¹ã‚¿ãƒƒã‚¯ãƒã‚¤ãƒ³ã‚¿ç¢ºèª
        printf("Stack Empty!\n");
        return -1;
    }else{
        data = stack[sp];   // ã‚¹ã‚¿ãƒƒã‚¯å€¤ã‚’ä»£å…¥
        sp--;               // ã‚¹ã‚¿ãƒƒã‚¯ãƒã‚¤ãƒ³ã‚¿ -1
        printf("Pop data is %d\n",data);
        return data;        // æ­£å¸¸çµ‚äº†
    }
